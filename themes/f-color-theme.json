{
  "name": "RL Themer",
  "tokenColors": [
    { "settings": { "foreground": "#670792" } },
    {
      "scope": ["meta.embedded", "source.groovy.embedded"],
      "settings": { "foreground": "#5cf614" }
    },
    {
      "name": "Comment",
      "scope": "comment",
      "settings": { "fontStyle": "italic", "foreground": "#d60ce1" }
    },
    {
      "name": "String",
      "scope": "string",
      "settings": { "foreground": "#6788ba" }
    },
    {
      "name": "Regexp",
      "scope": "string.regexp",
      "settings": { "foreground": "#a94b35" }
    },
    {
      "name": "Number",
      "scope": "constant.numeric",
      "settings": { "foreground": "#cb1b37" }
    },
    {
      "name": "Variable",
      "scope": ["variable.language", "variable.other"],
      "settings": { "foreground": "#9abe3e" }
    },
    {
      "name": "Keyword",
      "scope": "keyword",
      "settings": { "foreground": "#13e909" }
    },
    {
      "name": "Storage",
      "scope": "storage",
      "settings": { "fontStyle": "bold", "foreground": "#b9399f" }
    },
    {
      "name": "Class name",
      "scope": [
        "entity.name.class",
        "entity.name.type",
        "entity.name.namespace",
        "entity.name.scope-resolution"
      ],
      "settings": { "foreground": "#2fb9fb" }
    },
    {
      "name": "Function name",
      "scope": "entity.name.function",
      "settings": { "foreground": "#7769aa" }
    },
    {
      "name": "Variable start",
      "scope": "punctuation.definition.variable",
      "settings": { "foreground": "#5ac926" }
    },
    {
      "name": "Embedded code markers",
      "scope": [
        "punctuation.section.embedded.begin",
        "punctuation.section.embedded.end"
      ],
      "settings": { "foreground": "#01fd4e" }
    },
    {
      "name": "Built-in constant",
      "scope": ["constant.language", "meta.preprocessor"],
      "settings": { "foreground": "#16760e" }
    },
    {
      "name": "Support.construct",
      "scope": ["support.function.construct", "keyword.other.new"],
      "settings": { "foreground": "#ea979b" }
    },
    {
      "name": "User-defined constant",
      "scope": ["constant.character", "constant.other"],
      "settings": { "foreground": "#80d300" }
    },
    {
      "name": "Inherited class",
      "scope": "entity.other.inherited-class",
      "settings": {}
    },
    {
      "name": "Function argument",
      "scope": "variable.parameter",
      "settings": {}
    },
    {
      "name": "Tag name",
      "scope": "entity.name.tag",
      "settings": { "foreground": "#6e1789" }
    },
    {
      "name": "Tag start/end",
      "scope": [
        "punctuation.definition.tag.begin",
        "punctuation.definition.tag.end"
      ],
      "settings": { "foreground": "#519779" }
    },
    {
      "name": "Tag attribute",
      "scope": "entity.other.attribute-name",
      "settings": { "foreground": "#b79825" }
    },
    {
      "name": "Library function",
      "scope": "support.function",
      "settings": { "foreground": "#2f2a58" }
    },
    {
      "name": "Continuation",
      "scope": "punctuation.separator.continuation",
      "settings": { "foreground": "#675ddb" }
    },
    {
      "name": "Library constant",
      "scope": ["support.constant", "support.variable"],
      "settings": {}
    },
    {
      "name": "Library class/type",
      "scope": ["support.type", "support.class"],
      "settings": { "foreground": "#873391" }
    },
    {
      "name": "Library Exception",
      "scope": "support.type.exception",
      "settings": { "foreground": "#49cf5e" }
    },
    {
      "name": "Library variable",
      "scope": "support.other.variable",
      "settings": {}
    },
    { "name": "Invalid", "scope": "invalid", "settings": {} },
    {
      "name": "diff: header",
      "scope": ["meta.diff", "meta.diff.header"],
      "settings": { "fontStyle": "italic", "foreground": "#2cfce7" }
    },
    {
      "name": "diff: deleted",
      "scope": "markup.deleted",
      "settings": { "fontStyle": "", "foreground": "#de33a1" }
    },
    {
      "name": "diff: changed",
      "scope": "markup.changed",
      "settings": { "fontStyle": "", "foreground": "#0fd1d4" }
    },
    {
      "name": "diff: inserted",
      "scope": "markup.inserted",
      "settings": { "foreground": "#ca4d6e" }
    },
    {
      "name": "Markup Quote",
      "scope": "markup.quote",
      "settings": { "foreground": "#febc6f" }
    },
    {
      "name": "Markup Lists",
      "scope": "markup.list",
      "settings": { "foreground": "#50062a" }
    },
    {
      "name": "Markup Styling",
      "scope": ["markup.bold", "markup.italic"],
      "settings": { "foreground": "#c28df7" }
    },
    {
      "name": "Markup Inline",
      "scope": "markup.inline.raw",
      "settings": { "fontStyle": "", "foreground": "#83039d" }
    },
    {
      "name": "Markup Headings",
      "scope": "markup.heading",
      "settings": { "foreground": "#241ccc" }
    },
    {
      "name": "Markup Setext Header",
      "scope": "markup.heading.setext",
      "settings": { "fontStyle": "", "foreground": "#f6bc02" }
    }
  ],
  "colors": {
